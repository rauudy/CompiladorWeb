{
  "name": "@ts-jison/parser-generator",
  "description": "A lightly-typescriptified version of jison",
  "version": "0.4.1-alpha.2",
  "license": "MIT",
  "keywords": [
    "jison",
    "bison",
    "yacc",
    "parser",
    "generator",
    "lexer",
    "flex",
    "tokenizer",
    "compiler",
    "typescript"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/ericprud/ts-jison.git"
  },
  "bugs": {
    "url": "http://github.com/ericprud/ts-jison/issues"
  },
  "main": "lib/jison",
  "bin": {
    "ts-jison": "lib/cli.js"
  },
  "types999": "./lib/common.d.ts",
  "files": [
    "lib",
    "templates",
    "examples"
  ],
  "engines": {
    "node": ">=0.4"
  },
  "dependencies": {
    "@ts-jison/common-generator": "^0.4.1-alpha.1",
    "@ts-jison/ebnf-parser": "^0.4.1-alpha.1",
    "@ts-jison/lex-parser": "^0.4.1-alpha.1",
    "@ts-jison/lexer-generator": "^0.4.1-alpha.2",
    "@ts-jison/parser": "^0.4.1-alpha.1",
    "JSONSelect": "^0.4.0",
    "cjson": "^0.5.0",
    "escodegen": "^2.0.0",
    "esprima": "^4.0.1",
    "js-yaml": "^4.0.0",
    "nomnom": "1.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "echo nothing to build in parser-generator",
    "js-calculator-demo": "npm run generate:js-calculator && npm run cli:js-calculator",
    "generate:js-calculator": "(cd examples/ && ../lib/cli.js -n JsCalc -o js-calculator.js js-calculator.jison)",
    "run:js-calculator": "(cd examples/ && node js-calculator.js calculator.input.txt)",
    "cli:js-calculator": "(cd examples/ && node js-calculator.cli.js calculator.input.txt)",
    "ts-calculator-demo": "npm run generate:ts-calculator && npm run compile:ts-calculator && npm run cli:ts-calculator",
    "generate:ts-calculator": "(cd examples/ && ../lib/cli.js -t typescript -n TsCalc -o ts-calculator.ts ts-calculator.jison)",
    "compile:ts-calculator": "(cd examples/ && tsc ts-calculator.ts)",
    "run:ts-calculator": "(cd examples/ && node ts-calculator.js calculator.input.txt)",
    "cli:ts-calculator": "(cd examples/ && node ts-calculator.cli.js calculator.input.txt)",
    "ts-node-calculator-demo": "npm run generate:ts-calculator && npm run ts-node:ts-calculator",
    "ts-node:ts-calculator": "(cd examples/ && ts-node ts-calculator.cli.js calculator.input.txt)",
    "test": "node ./tests/all-tests"
  },
  "homepage": "https://github.com/ericprud/ts-jison/",
  "author": "Zach Carter",
  "contributors": [
    {
      "name": "Eric Prud'hommeauxh",
      "email": "eric@uu3.org",
      "url": "http://uu3.org/People/Eric/"
    }
  ],
  "gitHead": "22ce1cf2cf6bf59b63b8051de769834e43795e0a"
}
